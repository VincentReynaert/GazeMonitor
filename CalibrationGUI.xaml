<Window x:Class="GazeMonitor.CalibrationGUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        ResizeMode="NoResize" WindowStyle="None" WindowState="Maximized"
        KeyDown="Window_KeyDown" ContentRendered="Window_ContentRendered">
    <Grid x:Name="grid">
        <Canvas x:Name="canvas" Background="Black">
            <Path Fill="White">
                <Path.Data>
                    <EllipseGeometry x:Name="circleGeometry"/>
                </Path.Data>
            </Path>
        </Canvas>
    </Grid>
</Window>
